    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.84",
    "@types/node": "16.11.0",
    "@types/react-svg-radar-chart": "^1.2.3",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "concurrently": "6.5.1",
    "eslint": "8.0.1",
    "fuegojs": "^0.29.3",
    "prettier": "2.4.1",
    "ts-node": "10.3.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react-hooks": "2.2.2"
  }
